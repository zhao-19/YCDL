    <script>
    function success(data){
	    	
	    var state=data.status;//状态
	  
	    if(state==1){
            var name=data.name;
             var txt='感谢 <b>'+name+'</b> 的支持，我们会在两个工作日之内联系您商榷细节，盈创动力祝新年快乐 生意兴隆。';
             layer.alert(txt, {icon: 1, title : '提示'});
	    }else if(state==0){
        var errorMsg = '提交失败！';
        if(data.msg){
          errorMsg +=data.msg+'！';
        }
	        layer.alert(errorMsg, {icon: 2, title : '提示'});
	    }
    }
    var url="${pageContext.request.contextPath}/app/webproductapply.action";
    function ajaxweb(){
        if(check_message($("#productname")[0])){
            $.ajax({
                type: 'POST',
                url: url,
                data: $("#productname").serialize(),
                success: success,
                dataType: "json"
            });
        }
    }
