            });
        } else {
            $('.topnav').css({
                'height':'1.53rem',
                'paddingTop':'0.53rem'
            });
            $('.topnav>.flex').css({
                'height':'1rem'
            });
            $('.txt').css({
                'paddingTop':'4.2rem'
            });
        }
    });
    if ("${info}" == "0"){$('.topnav').hide();}
   
    var dsid='123';
    $('#dsback').click(function(){//返回
