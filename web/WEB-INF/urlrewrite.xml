<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.1//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.1.dtd">
<urlrewrite>
	 
	<!-- 分站 -->
	<rule>
		<from>/substation/([a-zA-Z0-9]+)/([a-zA-Z0-9]+).html</from>
		<to last="true">/substation!$1.action?subnum=$2</to>
	</rule>
	
 	<!-- 兼容DM单二维码首页 -->
	<rule>
		<from>/Default.html</from>
		<to last="true">index.jsp</to>
	</rule>
	 
	<!-- 会员中心 -->
	<rule>
